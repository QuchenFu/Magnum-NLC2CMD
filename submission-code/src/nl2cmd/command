onmt_build_vocab -config nl2cmd.yaml -n_sample 10347 --src_vocab_threshold 2 --tgt_vocab_threshold 2
onmt_train -config nl2cmd.yaml --tgt_words_min_frequency 2 --src_words_min_frequency 2
onmt_translate -model run/model_step_2000.pt -src invocations_proccess.txt -output pred_2000.txt -gpu 0 -verbose